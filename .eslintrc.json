{
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "rules": {
          "prettier/prettier": "error",
          "no-unused-vars": "warn"
    },
    "plugins": [
          "prettier"
    ]
}